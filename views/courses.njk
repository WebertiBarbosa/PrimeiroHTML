{% extends "layout.njk" %}


{% block courses %}
<body class="bootcamp">
    

    <header>
        <div class="menu">
            
            <div id="" class="links">
                <a href="/">Sobre</a>
                <a href="/courses">Conteúdos</a>
                <a href="https://discordapp.com/invite/gCRAFhc">Comunidade</a>
                <a href="mailto: oi@rocketseat.com.br">E-mail</a> 
                <a href="tel: +5547992078767">Telefone</a> 

                
            </div>
            <div class="redes">
                <a href="https://github.com/Rocketseat" target="_blank"><img src="github.svg" alt="github"></a>
                <a href="http://instagram.com/rocketseat_oficial" target="_blank"><img src="002-instagram-sketched.svg" alt="instagram"></a>
                <a href="http://fb.com/rocketseat" target="_blank"><img src="facebook.svg" alt="facebook"></a>
                
            </div>
        </div>

    </header>


    <section class="cards">
        <div class="card" id="starter">
            <div class="cards-image-container">
                <img src="starter.svg" alt="starter">
            </div>
            <div class="card-content">
                <p id="starter-p"><strong>Torne-se um programador desejado</strong></p>
                <p>no mercado com esses cursos</p>
                <p>gratuitos</p>
            </div>
            <div class="card-info">
                <p>05 Módulos</p>
                <p class="card-price">FREE</p>

            </div>
            
        </div>

        <div class="card" id="launchbase">
            <div class="cards-image-container">
                <img src="launchbase.svg" alt="LaunchBase">
            </div>
            <div class="card-content">
                <p id="launchbase-p"><strong>Domine a programação do zero</strong>
                <p>e tenha acesso às melhores </p>
                <p>oportunidades do mercado</p>
            </div>
            <div class="card-info">
                <p>06 Módulos</p>
                <p class="card-price">PAGO</p>

            </div>
            
        </div>

        <div class="card" id="gostack">
            <div class="cards-image-container">
                <img src="gostack.svg" alt="GoStack">
            </div>
            <div class="card-content">
                <p id="gostack-p"><strong>Treinamento imersivo</strong></p>
                <p>nas tecnologias mais modernas de</p>
                <p>desenvolvimento web e mobile</p>
            </div>
            <div class="card-info">
                <p>10 Módulos</p>
                <p class="card-price">PAGO</p>

            </div>
            
        </div>
    </section>

    <div class="modal-overlay">
        <div class="modal">
            <a class="close-modal">
                <i class="material-icons">close</i>
            </a>
            <a class="maximize-modal">
                <i class="material-icons">maximize</i>
            </a>
            
            <div class="modal-content">
                <iframe src="https://rocketseat.com.br/starter" frameborder="0"></iframe>

            </div>
        </div>

    </div>

    <script src="scripts.js"></script>

{% endblock %}


